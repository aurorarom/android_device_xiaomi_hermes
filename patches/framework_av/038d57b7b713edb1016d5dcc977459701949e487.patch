         }
 
         blks = 0;
-        for (v_blk = MB_V; v_blk < MB_V + MBSIZE; v_blk += BLKSIZE)
+        for (v_blk = MB_V; (unsigned int)v_blk < (unsigned int)((unsigned int)MB_V + (unsigned int)MBSIZE); v_blk += BLKSIZE)
         {
             v0 = v_blk - 1;
             for (h_blk = 0; h_blk < MBSIZE; h_blk += BLKSIZE)
